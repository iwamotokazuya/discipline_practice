<h1>録音画面2</h1>
<% if @user.images.attached? %>
    <% @user.images.each do |image| %>
      <%= image_tag image.variant(resize:'400x400').processed, class: 'img-responsive', id: "new-image" %>
    <% end %>
  <% end %>
<div class="wrapper">
  <section class="main-controls">
    <div id="buttons">
      <button class = "btn btn-dark w-25" id="record" >録音開始</button>
    </div>

</div>
<%= javascript_pack_tag 'audio' %>
